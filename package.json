{
  "name": "cra-template-complete-web-app",
  "version": "1.0.0",
  "description": "CRA template with: typescript, axios, sass, react-router, redux and much more already configured.",
  "homepage": ".",
  "license": "MIT",
  "main": "template.json",
  "files": [
    "template",
    "template.json"
  ],
  "devDependencies": {
